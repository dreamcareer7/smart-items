{

  "name": "Plain text",
  "tags": ["signpost", "text", "interactive"],
  "category": "decorations",
  "model": "models/signpost/Signpost_Golden.glb",
  "parameters": [
    {
      "id": "text",
      "label": "Text",
      "type": "textarea",
      "default": "Some text"
    },
    {
      "id": "fontSize",
      "label": "Font Size",
      "type": "slider",
      "min": 12,
      "max": 30,
      "step": 0.5,
      "default": 20
	},
	{
		"id": "font",
		"label" :"Font",
		"type": "options",
		"options": [
		  {
			"value": "SF",
			"label": "SanFrancisco"
		  },
		  {
			"value": "SF_Heavy",
			"label": "SanFrancisco Heavy"
		  }
		],
		"default": "SF"
	}
  ]
}
